
@PACKAGE_INIT@

set(ENABLE_CXXONLY "@ENABLE_CXXONLY@")
set(axom_DIR "@axom_DIR@" CACHE PATH "")
set(silo_DIR "@silo_DIR@" CACHE PATH "")
set(hdf5_DIR "@hdf5_DIR@" CACHE PATH "")
set(eigen_DIR "@eigen_DIR@" CACHE PATH "")
set(eigen_INCLUDES "@eigen_INCLUDES@" CACHE PATH "")
set(polytope_DIR "@polytope_DIR@" CACHE PATH "")
set(boost_DIR "@boost_DIR@" CACHE PATH "")
set(qhull_DIR "@qhull_DIR@" CACHE PATH "")

set(SPHERAL_INSTALL_PREFIX "@CMAKE_INSTALL_PREFIX@")
