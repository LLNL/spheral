	$(srcdir)/SynchronousRK4Inst.cc \
	$(srcdir)/CheapSynchronousRK2Inst.cc \
	$(srcdir)/VerletInst.cc

SPHERALTOP = $(srcdir)/..
ARCH = $(SPHERALARCH)

#-------------------------------------------------------------------------------
include $(SPHERALTOP)/helpers/makefile_master
